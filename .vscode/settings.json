{
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false,
        "**/Thumbs.db": false,
        "**/.pnpm-store": false,
        "**/node_modules": false,
        "**/*.tsbuildinfo": false,
        "**/dist": false,
        "**/build": false,
        "**/.svelte-kit": false,
        "**/.npmrc": false,
        "pnpm-lock.yaml": false
    },
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "tailwindCSS.files.exclude": [
        "**/.git/**",
        "**/node_modules/**",
        "**/.hg/**",
        "**/.svn/**",
        "packages/**",
        "scripts/**"
    ],
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "off",
        "strings": "on"
    },
    "svelte.enable-ts-plugin": true,
    "cSpell.words": [
        "sveltejs"
    ]
}
