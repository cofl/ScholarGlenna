{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.pnpm-store": true,
        "**/node_modules": true,
        "**/*.tsbuildinfo": true,
        "**/dist": true,
        "**/build": true,
        "**/.svelte-kit": true,
        "**/.npmrc": true,
        "pnpm-lock.yaml": true
    },
    "svelte.enable-ts-plugin": true,
